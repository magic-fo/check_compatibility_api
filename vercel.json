{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.8" }
    },
    {
      "src": "api/debug/test.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.8" }
    },
    {
      "src": "api/hello.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/hello",
      "dest": "api/hello.js"
    },
    {
      "src": "/debug/test",
      "dest": "api/debug/test.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
} 